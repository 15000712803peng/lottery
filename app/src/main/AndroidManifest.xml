<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cai.newc">

    <application
        android:name=".data.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ui.activity.HomeActivity"
                   android:launchMode="singleTop"></activity>

        <activity android:name=".ui.activity.ForgetPwdActivity"
            android:launchMode="singleTop"></activity>

        <activity android:name=".ui.activity.NewsActivity"
            android:launchMode="singleTop"></activity>

        <activity android:name=".ui.activity.SettingActivity"
                  android:launchMode="singleTop"></activity>

        <activity android:name=".ui.activity.EnterNumberActivity"
                  android:launchMode="singleTop"></activity>

        <activity android:name=".ui.activity.GetCodeActivity"
                  android:launchMode="singleTop"></activity>

        <activity android:name=".ui.activity.PersonalActivity"
                  android:launchMode="singleTop"></activity>

        <activity android:name=".ui.activity.OpinionActivity"
                  android:launchMode="singleTop"></activity>

        <activity android:name=".ui.activity.OpinionHistoryActivity"
                  android:launchMode="singleTop"></activity>

        <activity android:name=".ui.activity.RangeActivity"
                  android:launchMode="singleTop"></activity>

        <activity android:name=".ui.activity.ReviseNameActivity"
                  android:launchMode="singleTop"></activity>

        <activity android:name=".ui.activity.ReviseNumberActivity"
                  android:launchMode="singleTop"></activity>

        <activity android:name=".ui.activity.PointsActivity"
                  android:launchMode="singleTop"></activity>
    </application>



</manifest>
